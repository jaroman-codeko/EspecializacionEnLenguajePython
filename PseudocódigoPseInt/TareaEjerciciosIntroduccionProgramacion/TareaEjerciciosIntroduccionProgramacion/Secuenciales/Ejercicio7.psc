Proceso calcularNota
	Definir num,num2,nota,aux,aux2 Como real;
	
	Escribir "Hola introduzca su primera nota";
	Leer	num;
	Escribir "Introduzca su  nota deseada";
	Leer	num2;
	
	aux<- num * 0.4;
	aux2<-num2 -aux;
	nota<-aux2/0.6;

	
	
	
	Escribir "La nota  que necesita  en el segundo examen es de ",nota;

FinProceso
