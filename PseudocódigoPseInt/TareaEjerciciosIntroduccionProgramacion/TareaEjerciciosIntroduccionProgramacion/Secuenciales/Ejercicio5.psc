Proceso Media
	Definir num,num2 Como real;
	
	Escribir "Hola introduzca un número";
	Leer	num;
	Escribir "Hola introduzca un segundo número";
	Leer	num2;
	

	Escribir "La suma es  ",num+num2;
	Escribir "La resta es  ",num-num2;
	Escribir "La multiplicación es  ",num*num2;
	Escribir "La división es  ",num/num2;
FinProceso
